@model Bina.Models.ArticlesDeadline

@{
    ViewData["Title"] = "Details";
}

<h1 class="text-center mb-4">Articles Details</h1>

<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Term  Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TermName)
        </dd>

        <dt class="col-sm-2">
            Title
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TermTitle)
        </dd>

        <dt class="col-sm-2">
            Faculty ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FacultyId)
        </dd>

        <dt class="col-sm-2">
            Start Due
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StartDue)
        </dd>

        <dt class="col-sm-2">
            Due Date
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DueDate)
        </dd>
        <dt class="col-sm-2">
            Academic Year
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AcademicYear)
        </dd>
    </dl>
</div>
</br>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.ArticlesDeadlineId" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>